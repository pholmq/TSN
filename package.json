{
  "name": "the_tychosium",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pholmq/TSN.git"
  },
  "version": "1.0.2",
  "author": "Patrik Holmqvist",
  "description": "The Tychosium www.tychos.space",
  "license": "GPL-2.0-or-only",
  "homepage": "./",
  "main": "main.js",
  "dependencies": {
    "@react-three/drei": "9.102.6",
    "@react-three/fiber": "8.15.19",
    "@react-three/postprocessing": "2.19.0",
    "@types/three": "0.171.0",
    "fuse.js": "^7.1.0",
    "leva": "0.9.34",
    "lodash-es": "^4.17.21",
    "postprocessing": "6.35.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.4.0",
    "react-markdown": "^10.1.0",
    "react-scripts": "5.0.1",
    "three": "0.162.0",
    "zustand": "5.0.3"
  },
  "devDependencies": {
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.15",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^8.57.1",
    "prettier": "^2.8.8",
    "wait-on": "^9.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron:dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "dist": "npm run build && electron-builder -p always"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "build": {
    "appId": "com.tychosium.app",
    "extends": null,
    "extraMetadata": {
      "main": "main.js"
    },
    "files": [
      "build/**/*",
      "main.js",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "portable"
    }
  }
}
